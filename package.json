{
  "name": "classbro-light-weight-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cfg": "node ./script/generateConfig",
    "dev": "cross-env NODE_RUN=local vue-cli-service serve",
    "dev:test": "cross-env NODE_RUN=local vue-cli-service serve --mode testing",
    "dev:preview": "cross-env NODE_RUN=local vue-cli-service serve --mode preview",
    "prod": "cross-env NODE_RUN=local vue-cli-service serve --mode prod",
    "build:local": "vue-cli-service build --mode development",
    "build:test": "vue-cli-service build --mode testing",
    "build:preview": "vue-cli-service build --mode preview",
    "build": "vue-cli-service build --mode prod",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "0.19.2",
    "babel-polyfill": "6.26.0",
    "core-js": "3.6.5",
    "element-ui": "2.13.1",
    "js-base64": "^3.5.2",
    "js-cookie": "2.2.1",
    "lodash": "4.17.15",
    "moment": "2.27.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "svg-sprite-loader": "4.1.3",
    "vant": "2.9.0",
    "vue": "2.6.11",
    "vue-i18n": "8.17.4",
    "vue-router": "3.2.0",
    "vuedraggable": "^2.24.0",
    "vuex": "3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "~10.1.0",
    "cross-env": "~7.0.2",
    "eslint": "~6.7.2",
    "eslint-plugin-vue": "~6.2.2",
    "html-webpack-plugin": "~4.3.0",
    "sass": "~1.26.5",
    "sass-loader": "~8.0.2",
    "script-ext-html-webpack-plugin": "~2.1.4",
    "svgo": "1.2.0",
    "vue-template-compiler": "~2.6.11",
    "vue-uweb": "^0.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-useless-escape": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
